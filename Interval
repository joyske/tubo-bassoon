public class Interval
{
	int s;
	int e;

	public Interval(int start, int ende)
	{	
		
		
		if(start<=ende)
		{
			s=start;
			e=ende;
			
		}
		
		throw new IllegalArgumentException("Start kann nicht grÃ¶ÃŸer als Ende sein");
	}
	
	public int getStart()
	{
		return s;
	}
	public int getEnd()
	{
		return e;
	}
	public String toString()
	{
		String str="Das Intervall geht von"+ s + "bis" + e;
		return str;
	}
	
}

